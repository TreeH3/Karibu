@model Karibu.Models.Filiere

@{
    ViewData["Title"] = "Détails | Filière";
    ViewData["PageTitle"] = ViewData["Title"];
}

<div>
    <h4>Filiere</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nom)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nom)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdFiliere">Modifier</a> |
    <a asp-action="Index">Retour à la liste</a>
</div>
