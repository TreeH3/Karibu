@model Karibu.Models.Organe

@{
    ViewData["Title"] = "Supprimer | Organe";
    ViewData["PageTitle"] = ViewData["Title"];
}

<h3>Etes-vous sûr de vouloir supprimer ceci ?</h3>
<div>
    <h4>Organe</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomOrgane)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomOrgane)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdOrganeParentNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdOrganeParentNavigation.IdOrgane)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdTypeOrganeNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdTypeOrganeNavigation.IdTypeOrgane)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdOrgane" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour à la liste</a>
    </form>
</div>
